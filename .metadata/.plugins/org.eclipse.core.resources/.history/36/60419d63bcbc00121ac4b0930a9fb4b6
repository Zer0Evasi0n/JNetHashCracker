import javax.swing.*;

@SuppressWarnings("serial")
public class GUI extends JFrame{
	JPanel panelRate = new JPanel();
	JButton buttonRate = new JButton("Rate");

	public GUI() {
		panelRate.setLayout(null);
		panelRate.setLocation(360,10);
		panelRate.setSize(250,30);
		
		buttonRate.setLocation(0,0);
		buttonRate.setSize(100,30);
		//Add button to Panel
		panelRate.add(buttonRate);
		
		//Add Panel to Frame
		add(panelRate);
	}
	
	public static void init() {
		GUI gui = new GUI();
		gui.setLayout(null);
		gui.setTitle("J-Net Distributed MD5 Hash Cracker");
		gui.setSize(500,400);
		
		gui.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		gui.setVisible(true);
	}
}